        <Window x:Class="BarcodeCompareSystem.RePrint"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BarcodeCompareSystem"
        mc:Ignorable="d"
        Title="RePrint" Height="300" Width="300" FontSize="14" Background="#fafafa">
    <Border Background="#fafafa" CornerRadius="20" Margin="20">
            <StackPanel Margin="20">
            <Label Content="Start number" Foreground="Black"/>
            <TextBox Name="txtStartNumber" Background="White" Foreground="Black" FontSize="14" AcceptsReturn="True"/>
            <Label Content="Number label" Foreground="Black"/>
            <TextBox Name="txtNumberLabel" Background="White" Foreground="Black" FontSize="14" AcceptsReturn="True"/>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                <Button Name="btnRePrint" Content="RePrint"  Width="60" Margin="10" Click="btnReprint_Click"/>
                <Button Name="btnCancelRePrint" Content="Cancel" Width="60" Margin="10" Click="btnCancel_Click"/>
            </StackPanel>
         </StackPanel>
        </Border>
</Window>
